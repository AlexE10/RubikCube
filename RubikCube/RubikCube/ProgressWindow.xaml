<Window x:Class="RubikCube.Images.ProgressWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        WindowStartupLocation="CenterScreen"
        Title="Progress" Height="100" Width="300">
    <StackPanel>
        <ProgressBar x:Name="ProgressBar" Height="20" Margin="10"/>
        <Label x:Name="ProgressLabel" Content="Processing..." HorizontalAlignment="Center"/>
    </StackPanel>
</Window>
